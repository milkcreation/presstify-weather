{
  "name": "presstify-plugins/weather",
  "description": "Etension PresstiFy de gestion de widget météo.",
  "type": "presstify-plugin",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "weather"
  ],
  "homepage": "https://svn.tigreblanc.fr/presstify-plugins/weather",
  "time": "2019-11-13",
  "license": "proprietary",
  "authors": [
    {
      "name": "Louis Dherent",
      "email": "louis@tigreblanc.fr",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://composer.tigreblanc.fr/presstify/"
    }
  ],
  "require": {
    "cmfcmf/openweathermap-php-api": "^2.2",
    "composer/installers": "~1.0",
    "league/period": "^4.0",
    "presstify/framework": "^1.4"
  },
  "autoload": {
    "files": [
      "Helpers.php"
    ],
    "psr-4": {
      "tiFy\\Plugins\\Weather\\": ""
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
