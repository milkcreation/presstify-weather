{
  "name": "presstify-plugins/weather",
  "description": "Etension PresstiFy de gestion de widget météo.",
  "type": "presstify-plugin",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "weather"
  ],
  "homepage": "https://github.com/milkcreation/presstify-weather.git",
  "time": "2021-09-14",
  "authors": [
    {
      "name": "Jordy Manner", 
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "require": {
    "cmfcmf/openweathermap-php-api": "^2.2",
    "composer/installers": "~1.0",
    "league/period": "^4.0",
    "presstify/framework": "^1.4"
  },
  "autoload": {
    "files": [
      "Helpers.php"
    ],
    "psr-4": {
      "tiFy\\Plugins\\Weather\\": ""
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
